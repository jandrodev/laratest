{
    "name": "jandrodev/laratest",
    "description": "Test tools for your Laravel app",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jandrodev\\Laratest\\": "src/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Jandrodev\\Laratest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jandrodev\\Laratest\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jandrodev\\Laratest\\LaratestServiceProvider"
            ]
        }
    }
}
